  <system.webServer>
    <applicationInitialization>
      <add initializationPage="/wim.ashx" />
    </applicationInitialization>
    <rewrite>
      <!-- add HSTS -->
      <outboundRules>
        <rule name="Add Strict-Transport-Security when HTTPS" enabled="true">
          <match serverVariable="RESPONSE_Strict_Transport_Security" pattern=".*" />
          <conditions>
            <add input="{HTTPS}" pattern="on" ignoreCase="true" />
          </conditions>
          <action type="Rewrite" value="max-age=31536000" />
        </rule>
      </outboundRules>
      <rules>        
        <!--Enforce https-->
        <rule name="Force HTTPS">
          <match url="(.*)" ignoreCase="false" />
          <conditions>
            <add input="{HTTPS}" pattern="off" />
          </conditions>
          <action type="Redirect" url="https://{HTTP_HOST}{REQUEST_URI}" appendQueryString="true" redirectType="Permanent" />
        </rule>
      </rules>
    </rewrite>
    <httpProtocol>
      <customHeaders>
        <!-- Prevent iframes, except from ourselves (MK popups use iframe) -->
        <add name="X-Frame-Options" value="SAMEORIGIN" />
        <!-- Prevent XSS -->
        <add name="Content-Security-Policy" value="script-src 'self' data: 'unsafe-inline' 'unsafe-eval' use.fontawesome.com cdn.fontawesome.com ajax.googleapis.com fonts.googleapis.com az609478.vo.msecnd.net www.google-analytics.com cdnjs.cloudflare.com maxcdn.bootstrapcdn.com" />
        <add name="X-XSS-Protection" value="1" />
      </customHeaders>
    </httpProtocol>
    <modules>
      <add name="Wim.Framework.Relink" type="Wim.Framework.Relink, Wim.Framework" />
      <remove name="TelemetryCorrelationHttpModule" />
      <add name="TelemetryCorrelationHttpModule" type="Microsoft.AspNet.TelemetryCorrelation.TelemetryCorrelationHttpModule, Microsoft.AspNet.TelemetryCorrelation" preCondition="integratedMode,managedHandler" />
      <!--<remove name="ApplicationInsightsWebTracking" />
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" preCondition="managedHandler" />-->
    </modules>
    <urlCompression doStaticCompression="true" doDynamicCompression="true" />
    <httpErrors existingResponse="PassThrough" />
    <security>
      <requestFiltering>
        <!-- current max request size 5mb -->
        <requestLimits maxAllowedContentLength="5242880" />
      </requestFiltering>
    </security>
    <validation validateIntegratedModeConfiguration="false" />
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>

  <system.codedom>
